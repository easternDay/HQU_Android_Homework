<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FrameActivity">

    <FrameLayout
            android:layout_width="414dp"
            android:layout_height="0dp"
            android:id="@+id/Layout4Frame" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/Button4Back_Frame">
        <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FF6143" android:id="@+id/Image4Frame"/>

    </FrameLayout>
    <Button
            android:text="@string/back_linear_layout"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:id="@+id/Button4Back_Frame" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
    />
</androidx.constraintlayout.widget.ConstraintLayout>